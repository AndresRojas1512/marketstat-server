      "title": "IMPLEMENTATION 1: BASELINE (The Control Group)",
      "type": "row"
    },
    {
      "title": "Baseline: CPU Overhead (GC Time)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 1 },
      "targets": [
        {
          "expr": "rate(process_runtime_dotnet_gc_duration_nanoseconds_total{service_name=\"MarketStat.API.BASELINE\"}[10s])",
          "legendFormat": "GC Duration / Sec",
          "refId": "A",
          "color": "yellow"
        }
      ],
      "unit": "ns"
    },
    {
      "title": "Baseline: Memory Usage (Heap)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 1 },

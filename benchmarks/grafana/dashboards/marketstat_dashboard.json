        {
          "expr": "process_runtime_dotnet_gc_committed_memory_size_bytes{service_name=\"MarketStat.API.BASELINE\"}",
          "legendFormat": "Committed RAM",
          "refId": "A",
          "color": "red"
        }
      ],
      "unit": "bytes"
    },
    {
      "title": "Baseline: Reliability (Success vs Fail)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 1 },
      "targets": [
        {
          "expr": "sum(rate(http_server_request_duration_seconds_count{service_name=\"MarketStat.API.BASELINE\", http_response_status_code=~\"2..\"}[10s]))",
          "legendFormat": "Success (2xx)",
          "refId": "A",
          "color": "green"
        },
        {
          "expr": "sum(rate(http_server_request_duration_seconds_count{service_name=\"MarketStat.API.BASELINE\", http_response_status_code=~\"5..|4..\"}[10s]))",
          "legendFormat": "Errors",
